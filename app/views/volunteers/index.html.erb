<section class="back-img">

  <%= render 'volunteer_hero' %>

  <div class="container">
    <%= render 'volunteer_opportunities' %>

    <%= render 'requirements_benefits' %>

    <%= render 'google_connection_status' %>

    <!-- Schedule Your Volunteer Session -->
    <section class="schedule-section mb-5">
      <div class="schedule-container">
        <h2 class="text-center mb-4">Schedule Your Volunteer Session</h2>
        <%= render 'new', volunteer: @volunteer %>
      </div>
    </section>

    <hr class="my-5">

    <%= render 'sheduled_events' %>

    <%= render 'all_google_events' %>

  </div>
</section>
